@import "../../calendar/calendar.wxss";

.show-date {
    margin-top: 40rpx;
    text-align: center;
    font-size: 60rpx;
    font-weight: lighter;
}

.float-action {
  position: absolute;
  bottom: 40rpx;
  right: 40rpx;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  box-shadow: 2px 2px 10px rgba(0, 0, 0, .5);
  background: #E14848;
  z-index: 100;
}